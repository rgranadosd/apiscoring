{
  "name": "@inditextech/spa-apiscoringviewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clear": "pnpm run clear:parcel-cache & pnpm run clear:node_modules",
    "clear:node_modules": "rm -rf ./node_modules",
    "clear:parcel-cache": "rm -rf ./.parcel-cache",
    "build": "webpack --config webpack.config.js && npm run postbuild",
    "postbuild": "cp ./public/version.txt ./dist/version.txt",
    "dev": "webpack serve --config webpack.config.js --open",
    "kill-port": "kill-port ${PORT:-1234}",
    "test": "jest --config=jest.config.js"
  },
  "dependencies": {
    "@inditextech/apiscoringviewer": "*",
    "@mantine/core": "^6.0.15",
    "@mantine/notifications": "6.0.22",
    "kill-port": "latest",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-router-dom": "^5.3.4",
    "react-virtualized-auto-sizer": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.25.7",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.11",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "vite": "^7.0.5",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
